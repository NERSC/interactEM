{
  "id": null,
  "title": "Interactem Pipeline Monitoring",
  "tags": ["interactem", "prometheus", "pipeline"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "5s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Service Overview",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "id": 2,
      "title": "Service Status",
      "type": "stat",
      "targets": [
        {
          "expr": "interactem_service_status",
          "legendFormat": "{{service}}"
        }
      ],
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          },
          "mappings": [
            { "options": { "0": { "text": "Inactive" } }, "type": "value" },
            { "options": { "1": { "text": "Active" } }, "type": "value" }
          ]
        }
      }
    },
    {
      "id": 3,
      "title": "Service Uptime",
      "type": "stat",
      "targets": [
        {
          "expr": "interactem_service_uptime_seconds",
          "legendFormat": "{{service}}"
        }
      ],
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "id": 4,
      "title": "Active Pipelines",
      "type": "stat",
      "targets": [
        {
          "expr": "count(count by (pipeline_id) (interactem_pipeline_active_ports))",
          "legendFormat": "Pipelines"
        }
      ],
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 }
    },
    {
      "id": 5,
      "title": "Collection Errors",
      "type": "stat",
      "targets": [
        {
          "expr": "increase(interactem_metrics_collection_errors_total[5m])",
          "legendFormat": "{{error_type}}"
        }
      ],
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "id": 10,
      "title": "Port Metrics",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "collapsed": false
    },
    {
      "id": 11,
      "title": "Port Throughput (Send)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "interactem_port_throughput_send_mbps",
          "legendFormat": "{{port_id}} ({{operator_type}}): {{interval}}"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "fieldConfig": {
        "defaults": {
          "unit": "Mbps"
        }
      }
    },
    {
      "id": 12,
      "title": "Port Throughput (Receive)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "interactem_port_throughput_recv_mbps",
          "legendFormat": "{{port_id}} ({{operator_type}}): {{interval}}"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "fieldConfig": {
        "defaults": {
          "unit": "Mbps"
        }
      }
    },
    {
      "id": 13,
      "title": "Message Rate (Send)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "interactem_port_message_rate_send",
          "legendFormat": "{{port_id}} ({{operator_type}}): {{interval}}"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "fieldConfig": {
        "defaults": {
          "unit": "msg/s"
        }
      }
    },
    {
      "id": 14,
      "title": "Message Rate (Receive)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "interactem_port_message_rate_recv",
          "legendFormat": "{{port_id}} ({{operator_type}}): {{interval}}"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "fieldConfig": {
        "defaults": {
          "unit": "msg/s"
        }
      }
    },
    {
      "id": 15,
      "title": "Total Messages Sent",
      "type": "timeseries",
      "targets": [
        {
          "expr": "interactem_port_messages_sent_total",
          "legendFormat": "{{port_id}} ({{operator_type}})"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 22 },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 16,
      "title": "Total Messages Received",
      "type": "timeseries",
      "targets": [
        {
          "expr": "interactem_port_messages_received_total",
          "legendFormat": "{{port_id}} ({{operator_type}})"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 22 },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 20,
      "title": "Edge Metrics",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 30 },
      "collapsed": false
    },
    {
      "id": 21,
      "title": "Edge Message Difference",
      "type": "timeseries",
      "targets": [
        {
          "expr": "interactem_edge_message_diff",
          "legendFormat": "{{input_port_id}} → {{output_port_id}}"
        }
      ],
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 31 },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 22,
      "title": "Edge Byte Difference",
      "type": "timeseries",
      "targets": [
        {
          "expr": "interactem_edge_byte_diff",
          "legendFormat": "{{input_port_id}} → {{output_port_id}}"
        }
      ],
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 31 },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "id": 23,
      "title": "Edge Throughput Difference",
      "type": "timeseries",
      "targets": [
        {
          "expr": "interactem_edge_throughput_diff_mbps",
          "legendFormat": "{{input_port_id}} → {{output_port_id}}"
        }
      ],
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 31 },
      "fieldConfig": {
        "defaults": {
          "unit": "Mbps"
        }
      }
    },
    {
      "id": 30,
      "title": "Operator Performance",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 39 },
      "collapsed": false
    },
    {
      "id": 34,
      "title": "Operator Processing Time (Latest/Real-time)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "interactem_operator_processing_time_latest_microseconds",
          "legendFormat": "{{operator_id}} ({{operator_type}})"
        }
      ],
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 40 },
      "fieldConfig": {
        "defaults": {
          "unit": "µs",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false,
            "showPoints": "always",
            "pointSize": 5,
            "stacking": {
              "mode": "none"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10000 },
              { "color": "red", "value": 100000 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["last", "mean", "max"]
        }
      }
    },
    {
      "id": 31,
      "title": "Operator Processing Time (P95)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(interactem_operator_processing_time_histogram_microseconds_bucket[5m]))",
          "legendFormat": "{{operator_id}} ({{operator_type}})"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 48 },
      "fieldConfig": {
        "defaults": {
          "unit": "µs"
        }
      }
    },
    {
      "id": 32,
      "title": "Operator Processing Time (Average)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(interactem_operator_processing_time_histogram_microseconds_sum[5m]) / rate(interactem_operator_processing_time_histogram_microseconds_count[5m])",
          "legendFormat": "{{operator_id}} ({{operator_type}})"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 48 },
      "fieldConfig": {
        "defaults": {
          "unit": "µs"
        }
      }
    },
    {
      "id": 33,
      "title": "Processing Time Distribution",
      "type": "heatmap",
      "targets": [
        {
          "expr": "rate(interactem_operator_processing_time_histogram_microseconds_bucket[5m])",
          "legendFormat": "{{le}}"
        }
      ],
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 56 }
    },
    {
      "id": 40,
      "title": "Pipeline Status",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 64 },
      "collapsed": false
    },
    {
      "id": 41,
      "title": "Active Ports per Pipeline",
      "type": "timeseries",
      "targets": [
        {
          "expr": "interactem_pipeline_active_ports",
          "legendFormat": "{{pipeline_id}}"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 65 },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 42,
      "title": "Active Operators per Pipeline",
      "type": "timeseries",
      "targets": [
        {
          "expr": "interactem_pipeline_active_operators",
          "legendFormat": "{{pipeline_id}}"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 65 },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 50,
      "title": "System Metrics",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 73 },
      "collapsed": false
    },
    {
      "id": 51,
      "title": "Metrics Collection Duration",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(interactem_metrics_collection_duration_seconds_bucket[5m]))",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.50, rate(interactem_metrics_collection_duration_seconds_bucket[5m]))",
          "legendFormat": "P50"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 74 },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 52,
      "title": "Collection Error Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(interactem_metrics_collection_errors_total[5m])",
          "legendFormat": "{{error_type}}"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 74 },
      "fieldConfig": {
        "defaults": {
          "unit": "errors/s"
        }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "pipeline_id",
        "type": "query",
        "query": "label_values(interactem_pipeline_active_ports, pipeline_id)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*"
      },
      {
        "name": "operator_type",
        "type": "query",
        "query": "label_values(interactem_port_messages_sent_total, operator_type)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*"
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "name": "Pipeline Events",
        "datasource": "Prometheus",
        "expr": "changes(interactem_pipeline_active_operators[1m]) > 0",
        "titleFormat": "Pipeline Change",
        "textFormat": "Active operators changed for {{pipeline_id}}"
      }
    ]
  }
}
